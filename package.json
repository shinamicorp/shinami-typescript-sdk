{
  "name": "shinami-typescript-sdk",
  "description": "Workspace root. Not to be published",
  "private": true,
  "workspaces": [
    "packages/clients",
    "packages/legacy",
    "packages/nextjs-zklogin",
    "examples/nextjs-zklogin"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "lint": "prettier --check . && npm run lint --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "integration": "npm run integration --workspaces --if-present",
    "coverage": "npm run coverage --workspaces --if-present",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
